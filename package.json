{
  "name": "hubspot-renewal-health-usage-app",
  "version": "1.0.0",
  "description": "Installable HubSpot App that ingests product-usage data from your SaaS platform, calculates renewal health scores for each customer account, and surfaces actionable churn-risk insights directly inside HubSpot. Helps CS and revenue teams predict renewals, identify at-risk accounts, and drive expansion.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "hubspot",
    "crm",
    "renewal",
    "health-score",
    "usage-analytics",
    "churn-prediction"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hubspot/api-client": "^13.4.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
